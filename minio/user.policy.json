{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Action": [
          "admin:ServerInfo"
        ],
        "Effect": "Allow",
        "Sid": ""
      },
      {
        "Action": [
          "s3:ListenBucketNotification",
          "s3:PutBucketNotification",
          "s3:GetBucketNotification",
          "s3:ListMultipartUploadParts",
          "s3:ListBucketMultipartUploads",
          "s3:ListBucket",
          "s3:HeadBucket",
          "s3:GetObject",
          "s3:GetBucketLocation",
          "s3:AbortMultipartUpload",
          "s3:CreateBucket",
          "s3:PutObject",
          "s3:DeleteObject",
          "s3:DeleteBucket",
          "s3:PutBucketPolicy",
          "s3:DeleteBucketPolicy",
          "s3:GetBucketPolicy"
        ],
        "Effect": "Allow",
        "Resource": [
          "arn:aws:s3:::*"
        ],
        "Sid": ""
      }
    ]
  }