{
  "node_name": "consul_agent",
  "server": false,
  "datacenter": "labqs",
  "data_dir": "/consul/data",
  "log_level":"INFO",
  "retry_join":[ "consul" ],
  "service": {
    "id": "dns",
    "name": "dns",
    "address": "localhost",
    "port": 8600,
    "enable_tag_override": false,
    "check": {
      "id": "dns",
      "name": "Consul DNS TCP on port 8600",
      "tcp": "localhost:8600",
      "interval": "10s",
      "timeout": "1s"
    }
  },
  "acl": {
    "enabled": true,
    "default_policy": "deny",
    "enable_token_persistence": true
  }
}
